{
  "name": "platomformio",
  "main": "./lib/platomformio",
  "version": "0.7.0",
  "description": "Integration with PlatformIO an open source ecosystem for IoT development. Cross-platform code builder and library manager. Continuous and IDE integration. Arduino and MBED compatible. Ready for Cloud compiling.",
  "keywords": [
    "PlatformIO",
    "IoT",
    "embedded",
    "cross",
    "arduino",
    "mbed",
    "cli",
    "builder",
    "library manager",
    "esp8266"
  ],
  "activationCommands": {
    "atom-workspace": [
      "platomformio:build",
      "platomformio:upload",
      "platomformio:program",
      "platomformio:uploadfs",
      "platomformio:clean",
      "platomformio:close",
      "platomformio:kill"
    ]
  },
  "repository": "https://github.com/vigetlabs/platomformio",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "ansi-to-html": ">0.1.0",
    "atom-space-pen-views": "^2.0.3"
  }
}
