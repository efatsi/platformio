{
  "name": "platomformio",
  "main": "./lib/platomformio",
  "version": "0.5.0",
  "description": "Integration with PlatformIO: a cross-platform code builder and the missing library manager",
  "keywords": [
    "PlatformIO",
    "embedded",
    "cross",
    "arduino",
    "mbed",
    "cli"
  ],
  "activationCommands": {
    "atom-workspace": [
      "platomformio:build",
      "platomformio:upload",
      "platomformio:clean",
      "platomformio:close",
      "platomformio:kill"
    ]
  },
  "repository": "https://github.com/vigetlabs/platomformio",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "ansi-to-html": ">0.1.0",
    "atom-space-pen-views": "^2.0.3"
  }
}
